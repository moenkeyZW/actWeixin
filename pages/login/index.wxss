/* pages/login/index.wxss */

.picture {
  background: #f2f2f2;
  position: fixed;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center; /*垂直居中*/
  justify-content: center; /*水平居中*/
}

.picture>image {
  width: 100%;
  height: 480rpx;
  margin-bottom: 50rpx;
}

.goAuth>button {
  background: #19b4f2;
  width: 215rpx;
  color: #fff;
  border-radius: 10rpx;
  font-size: 33rpx;
}
